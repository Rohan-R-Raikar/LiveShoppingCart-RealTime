@model List<LiveShoppingCart_RealTime.Models.ApplicationUser>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>
                    <a asp-action="AssignRoles" asp-route-userId="@user.Id">Assign Roles</a>
                </td>
            </tr>
        }
    </tbody>
</table>
